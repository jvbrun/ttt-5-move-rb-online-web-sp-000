#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here
puts "Welcome to Tic Tac Toe!"
board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]
puts "Where would you like to go?"
input = gets.strip

puts board.inspect

input_to_index(input)
move(board, input_to_index, value = "X")

puts display_board(board)
puts board.inspect